<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Presentation">


    <ImageButton
        android:id="@+id/helpButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="16dp"
        android:background="@color/transparent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_help" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="189dp"
        android:layout_height="72dp"
        android:layout_alignEnd="@+id/prayImage"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/prayImage"
        app:srcCompat="@drawable/gdc"
        android:contentDescription="@string/gdc"
        android:layout_marginTop="45dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        />
    <TextView
        android:id="@+id/editText3"
        android:layout_width="185dp"
        android:layout_height="19dp"
        android:layout_below="@+id/imageView5"
        android:layout_centerHorizontal="true"
        android:ems="10"
        android:textAlignment="center"
        android:text="@string/gdc_long"
        android:fontFamily="Lobster,cursive"
        android:layout_marginTop="0dp"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        />



    <ImageView
        android:id="@+id/readingBibleImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView5"
        android:layout_centerHorizontal="true"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/bible2"
        android:contentDescription="@string/open_bible"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText3"
         />
    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignStart="@+id/readingBibleImage"
        android:layout_alignLeft="@+id/readingBibleImage"
        android:layout_below="@+id/readingBibleImage"
        android:ems="10"
        android:text="@string/read_title"
        android:textAlignment="center"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="0dp"
        app:layout_constraintTop_toBottomOf="@+id/readingBibleImage"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        />


    <ImageView
        android:id="@+id/sharingImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView5"
        android:layout_marginTop="0dp"
        android:layout_marginBottom="6dp"
        android:layout_marginRight="0dp"
        android:layout_marginEnd="0dp"
        android:contentDescription="@string/women_chatting"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:srcCompat="@drawable/share2" />
    <TextView
        android:id="@+id/sharingTitleText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignStart="@+id/sharingImage"
        android:layout_alignLeft="@+id/sharingImage"
        android:layout_below="@+id/sharingImage"
        android:ems="10"
        android:text="@string/share_title"
        android:textAlignment="center"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sharingImage" />


    <ImageView
        android:id="@+id/prayImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editText3"
        app:srcCompat="@drawable/pray2"
        android:contentDescription="@string/man_who_prays"
        android:layout_marginBottom="6dp"
        android:layout_marginTop="0dp"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/prayImage"
        android:layout_alignStart="@+id/prayImage"
        android:layout_below="@+id/prayImage"
        android:layout_marginRight="8dp"
        android:ems="10"
        android:text="@string/pray_title"
        android:textAlignment="center"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/prayImage" />





</android.support.constraint.ConstraintLayout>

<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="gdc.sud.ffja.advantiste.org.mygdcapplication.Presentation">


    <RelativeLayout
        android:layout_width="368dp"
        android:layout_height="551dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="8dp">

        <ImageView
            android:id="@+id/readingBibleImage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView6"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="60dp"
            android:layout_marginLeft="240dp"
            android:layout_marginRight="0dp"
            android:layout_marginTop="500dp"
            app:srcCompat="@drawable/bible"
            tools:layout_editor_absoluteX="31dp"
            tools:layout_editor_absoluteY="261dp" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="60dp"
            android:layout_marginLeft="120dp"
            android:layout_marginRight="120dp"
            android:adjustViewBounds="true"
            app:srcCompat="@drawable/dispatch12"
            tools:layout_editor_absoluteX="148dp"
            tools:layout_editor_absoluteY="292dp" />

        <ImageView
            android:id="@+id/sharingImage"
            android:layout_width="136dp"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/partage"
            tools:layout_editor_absoluteX="257dp"
            tools:layout_editor_absoluteY="272dp"
            android:layout_below="@+id/readingBibleImage"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageView
            android:id="@+id/prayImage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/pray"
            tools:layout_editor_absoluteX="141dp"
            tools:layout_editor_absoluteY="96dp"
            android:layout_marginTop="10dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" />
    </RelativeLayout>

</android.support.constraint.ConstraintLayout>
